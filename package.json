{
  "name": "callstats",
  "preferGlobal": true,
  "version": "1.2.0",
  "description": "A tiny JavaScript library using WebRTC getStats API to return peer connection stats i.e. bandwidth usage, packets lost, local/remote ip addresses and ports, type of connection etc.",
  "scripts": {
    "start": "node server.js"
  },
  "main": "./callStats.js",
  "types": "./callStats.d.ts",
  "keywords": [
    "webrtc",
    "getStats",
    "callstats",
    "webrtc-library",
    "library",
    "javascript",
    "rtcweb",
    "webrtc-experiment",
    "javascript-library"
  ],
  "analyze": false,
  "readmeFilename": "README.md",
  "tonicExampleFilename": "npm-test.js",
  "devDependencies": {
    "grunt": "0.4.5",
    "grunt-bump": "0.7.0",
    "grunt-cli": "0.1.13",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "0.5.1",
    "grunt-contrib-obfuscator": "^6.0.0",
    "grunt-contrib-uglify": "0.11.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-jsbeautifier": "0.2.10",
    "javascript-obfuscator": "^0.28.5",
    "load-grunt-tasks": "3.4.0"
  }
}
